{
  "name": "agent",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "exports": {
    "./submodule": "./src/utils/hardware-utils.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
