    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "axios": "^1.6.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [ "node18-macos-arm64" ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node-os-utils": "^1.3.4",
